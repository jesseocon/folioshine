<!DOCTYPE html>
<html>
<head>
  <title>Folioshine</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <script src="http://js.pusher.com/2.0/pusher.min.js"></script>
  <%= csrf_meta_tags %>
</head>
<body>
<%= render 'layouts/signup_header' %>
</body>
</html>
<div class="container" id="account-page-content">
  <div class="row">
    <div class="span12">
      <div class="parent-wrapper">
        <ul class="inline navbar-list">
          <li>
            <btn class="btn btn-custom nav-button" data-target="<%= albums_path %>">
              <img src="https://s3-us-west-2.amazonaws.com/folioshine-icons/HomeBtn.png"> &nbsp;Home</btn>
          </li><!-- end of li -->
          <li>
            <btn class="btn btn-custom nav-button" data-target="<%= new_album_path %>">
              <img src="https://s3-us-west-2.amazonaws.com/folioshine-icons/CreateFolio1.png"> &nbsp;Create Folio</btn>
          </li><!-- end of li-->
          <li>
            <btn class="btn btn-custom" disabled='disabled'>
              <img src="https://s3-us-west-2.amazonaws.com/folioshine-icons/FindFolioButton.png"> &nbsp;Find a Folio</btn>
          </li><!-- end of li-->
          <li>
            <btn class="btn btn-custom" disabled='disabled'>
              <img src="https://s3-us-west-2.amazonaws.com/folioshine-icons/FindMemberBtn.png"> Find A Friend</btn>
          </li><!-- end of li-->
          <li>
            <btn class="btn btn-custom" disabled='disabled'>
              <img src="https://s3-us-west-2.amazonaws.com/folioshine-icons/HowItWorksBtn.png"> How It Works</btn>
          </li><!-- end of li-->
        </ul><!-- end of ul inline navbar-list -->
      </div><!-- end parent wrapper -->
    </div><!-- end of span12 -->
  </div><!-- end of row -->
  <div class="container" id="account-pages">
    <div class="row sidebar-and-content">
      <div class="span3">
        <div class="parent-wrapper">
          <ul class="sidebar unstyled">
            <li class="active">
              <btn class="btn btn-custom btn-sidebar" disabled='disabled'>My Profile</btn>
            </li><!-- end of li-->
            <li>
              <btn class="btn btn-custom btn-sidebar nav-button" data-target="<%= albums_path %>">My Folios</btn>
            </li><!-- end of li-->
            <li>
              <btn class="btn btn-custom btn-sidebar" disabled='disabled'>Members I Follow</btn>
            </li><!-- end of li-->
            <li>
              <btn class="btn btn-custom btn-sidebar" disabled='disabled'>Folios I Follow</btn>
            </li><!-- end of li-->
            <li>
              <btn class="btn btn-custom btn-sidebar" disabled='disabled'>Photos I Like</btn>
            </li><!-- end of li-->
          </ul><!-- end of ul -->
        </div><!-- end of parent-wrapper -->
      </div><!-- end of span3 -->
      <div class="span9 top-spacer">
        <div class="row">
			<%= yield %>
        </div><!-- end row -->
      </div><!-- end span9 top-spacer -->
    </div><!-- end row sidebar-and-content -->
  </div><!-- end container account-pages -->
</div><!-- end container account-page-content -->